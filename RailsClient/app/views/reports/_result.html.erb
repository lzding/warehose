<table class="table table-striped table-condensed table-hover">
  <thead>
  <tr>
    <th>No.</th>
    <th>零件号</th>
    <th>数量</th>
    <th>箱数</th>
    <th>部门</th>
    <th>状态</th>
    <th>时间</th>
  </tr>
  </thead>
  <tbody>
  <%@packages.each_with_index do |p,index|%>
  <tr>
    <td><%= index+1%></td>
    <td><%= p['part_id']%></td>
    <td><%= p['count']%></td>
    <td><%= p['box']%></td>
    <td><%= p['whouse']%></td>
    <td><%= MovableState.display(p['state'])%></td>
    <td><%= DatetimeHelper.ddate(p['ddate']) %></td>
  </tr>
  <%end%>
  </tbody>
</table>

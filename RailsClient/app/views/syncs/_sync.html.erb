<table class="table table-striped table-condensed table-hover">
  <thead>
  <tr>
    <th>数据表</th>
    <th>是否正在同步</th>
    <th>更新时间</th>
  </tr>
  </thead>
  <tbody>
  <% @sync_logs.all.each do |sl|%>
      <tr>
        <td><%= sl.table_name%></td>
        <td><%= sl.sync%></td>
        <td><%= sl.updated_at.localtime.strftime("%Y-%m-%d %H:%M:%S")%></td>
      </tr>
  <% end %>
  </tbody>
</table>